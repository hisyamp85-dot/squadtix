{
  "typescript": true,
  "directories": {
    "app": "app",
    "config": "config",
    "contracts": "contracts",
    "providers": "providers",
    "database": "database",
    "public": "public",
    "resources": "resources",
    "start": "start",
    "tests": "tests"
  },
  "preloads": [
    "./start/kernel",
    "./start/routes"
  ],
  "providers": [
    "@adonisjs/core",
    "@adonisjs/lucid",
    "@adonisjs/events",
    "@adonisjs/auth"
  ],
  "aceProviders": [
    "@adonisjs/lucid",
    "@adonisjs/events"
  ],
  "aliases": {
    "App": "app",
    "Config": "config",
    "Contracts": "contracts",
    "Database": "database",
    "HttpContext": "Adonis/Core/HttpContext",
    "Route": "Adonis/Core/Route",
    "Event": "Adonis/Core/Event"
  },
  "commands": [
    "./commands",
    "@adonisjs/lucid/build/commands"
  ],
  "metaFiles": [
    {
      "pattern": "public/**",
      "reloadServer": false
    },
    {
      "pattern": "resources/**",
      "reloadServer": false
    }
  ]
}
