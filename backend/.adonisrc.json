{
  "typescript": true,
  "directories": {
    "app": "app",
    "config": "config",
    "contracts": "contracts",
    "providers": "providers",
    "database": "database",
    "public": "public",
    "resources": "resources",
    "start": "start",
    "tests": "tests"
  },
  "providers": [
    "@adonisjs/core",
    "@adonisjs/lucid",
    "@adonisjs/events"
  ],
  "commands": [
    "./commands",
    "@adonisjs/lucid/build/commands"
  ],
  "metaFiles": [
    {
      "pattern": "public/**",
      "reloadServer": false
    },
    {
      "pattern": "resources/**",
      "reloadServer": false
    }
],
  "aceProviders": [
    "@adonisjs/lucid",
    "@adonisjs/events"
  ],
  "aliases": {
    "App": "App",
    "Route": "Adonis/Core/Route",
    "Event": "Adonis/Core/Event"
  }
}
